    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "S3-JSON-Logger Lambda Handler",
            "invokeTarget": {
                "target": "code",
                "lambdaHandler": "./src/handlers/s3-json-logger.s3JsonLoggerHandler",
                "projectRoot": "${workspaceFolder}"
            },
            "aws": {
                "region": "us-east-1",
                "credentials": "profile:cis-dev-ecs-devteam-user"
            },
            "lambda": {
                "runtime": "nodejs18.x",
                "payload": {
                    "json": {
                        "Records": [
                            {
                                "eventVersion": "2.0",
                                "eventSource": "aws:s3",
                                "awsRegion": "us-east-1",
                                "eventTime": "1970-01-01T00:00:00.000Z",
                                "eventName": "ObjectCreated:Put",
                                "userIdentity": {
                                    "principalId": "EXAMPLE"
                                },
                                "requestParameters": {
                                    "sourceIPAddress": "127.0.0.1"
                                },
                                "responseElements": {
                                    "x-amz-request-id": "EXAMPLE123456789",
                                    "x-amz-id-2": "EXAMPLE123/5678abcdefghijklambdaisawesome/mnopqrstuvwxyzABCDEFGH"
                                },
                                "s3": {
                                    "s3SchemaVersion": "1.0",
                                    "configurationId": "testConfigRule",
                                    "bucket": {
                                        "name": "simple-app-bucket",
                                        "ownerIdentity": {
                                            "principalId": "EXAMPLE"
                                        },
                                        "arn": "arn:aws:s3:::aws-us-east-1-777957353822-tricklercache-simpleappbucket"
                                    },
                                    "object": {
                                        "key": "test/key",
                                        "size": 1024,
                                        "eTag": "0123456789abcdef0123456789abcdef",
                                        "sequencer": "0A1B2C3D4E5F678901"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/handlers/s3-json-logger.ts"
        }
    ]
}